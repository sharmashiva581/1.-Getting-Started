import java.util.*;

public class Main{

public static void main(String[] args) {
  // write your code here  
  Scanner scn = new Scanner(System.in);
  int num= scn.nextInt();
  
   int ans = 0;
  
     int fv = 1;
    while( num > 0){// fv = face value
        int digit = num % 10;
        num = num/10;
        ans += fv * (int)Math.pow(10 , digit-1);
        fv +=1;
    }
       
  
  System.out.println(ans);
 }
}
